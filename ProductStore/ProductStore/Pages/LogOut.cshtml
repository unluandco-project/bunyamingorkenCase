@page
@model ProductStore.Pages.LogOutModel
@{
}
<form method="post">
    <div class="container mt-5">
        <div class="row justify-content-center align-items-center">
            <div class="col-sm-12 col-md-12 col-lg-4">
                <p>
                    Are You Sure To LogOut?
                </p>
                <p>
                    <button class="btn btn-success" asp-page-handler="LogOut">Yes</button>
                    <button class="btn btn-danger" asp-page-handler="DontLogOut">No</button>
                </p>
            </div>
        </div>
    </div>
</form>
